@model CircleSpaceGeneralModels.Models.PageModel

<div class="pageBox">
    <table style="width:100%">
        <tr>
            <th>Contributors</th>
            <th>Emails</th>
            <th>Route</th>
         </tr>
        <tr>
            <td>
                 @foreach (var contributors in Model.Contributors)
                {
                    @contributors.Username
                }

            </td>
            <td>
                @foreach (var contributors in Model.Contributors)
                {
                    @contributors.Email
                }
            </td>
            <td>@Model.Route</td>
         </tr>
    </table>

</div>
<div class="layoutBox">
    <table style="width:100%">
        <tr></tr>
    </table>
</div>